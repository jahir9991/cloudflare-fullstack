{
	"name": "cloudflare-fullstack",
	"version": "0.0.1",
	"license": "Apache-2.0",
	"scripts": {
		"start": "vite dev",
		"proxy": "wrangler dev node_modules/cfw-bindings-wrangler-bridge/worker/index.js --remote --env staging --ip 127.0.0.1 --port 8787",
		"build": "vite build",
		"build:prod": "npm run  db:prod:push:all && vite build",
		"db:prod:push:all": "npm run supabase:prod && npm run neon:prod  ",
		"neon:local": "drizzle-kit generate:pg --config=drizzleNeon.config.ts",
		"neon:prod": "drizzle-kit push:pg --config=drizzleNeon.config.ts",
		"supabase:local": "drizzle-kit generate:pg --config=drizzleSupabase.config.ts",
		"supabase:prod": "drizzle-kit push:pg --config=drizzleSupabase.config.ts",
		"d1:gen": "drizzle-kit generate:sqlite --config=drizzleD1.config.ts",
		"d1:mig": "drizzle-kit push:sqlite  --config=drizzleD1.config.ts",
		"d1:mig:local": "wrangler d1 migrations apply cloudflare_fullstack_db --local",
		"d1:mig:prod": "wrangler d1 migrations apply cloudflare_fullstack_db ",
		"preview": "vite preview",
		"w": "wrangler pages dev ./.svelte-kit/cloudflare  --env staging  ",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@envelop/graphql-jit": "^8.0.1",
		"@fontsource/fira-mono": "^4.5.10",
		"@graphql-yoga/plugin-response-cache": "^3.2.1",
		"@graphql-yoga/render-graphiql": "^5.1.1",
		"@neoconfetti/svelte": "^1.0.0",
		"@neondatabase/serverless": "^0.6.0",
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.1.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"autoprefixer": "^10.4.15",
		"bcryptjs": "^2.4.3",
		"cfw-bindings-wrangler-bridge": "^0.10.1",
		"chart.js": "^4.4.1",
		"cssnano": "^6.0.2",
		"currency.js": "^2.0.4",
		"dayjs": "^1.11.10",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.10",
		"drizzle-orm": "^0.29.3",
		"drizzle-zod": "^0.5.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"export-to-csv": "^1.2.2",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"graphql": "^15.8.0",
		"graphql-yoga": "^5.1.1",
		"jose": "^4.14.4",
		"jwt-decode": "^3.1.2",
		"mdsvex": "^0.11.0",
		"nanoid": "^4.0.2",
		"postcss": "^8.4.33",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"postgres": "^3.4.3",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.8.1",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-flatpickr": "^3.3.4",
		"svelte-french-toast": "^1.2.0",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "^3.4.0",
		"tailwindcss-animated": "^1.0.1",
		"tslib": "^2.6.2",
		"tsyringe": "^4.8.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vitest": "^1.0.0",
		"wrangler": "^3.22.4"
	},
	"type": "module"
}
